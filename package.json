{
  "name": "eslint-config-sweet",
  "version": "16.0.2",
  "description": "ESLint shareable config for the most sweet-ass vanilla JavaScript. 🤙🏼",
  "main": "./configs/javascript.cjs",
  "exports": {
    ".": "./configs/javascript.cjs",
    "./typescript": "./configs/typescript.cjs"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": "https://github.com/robinloeffel/eslint-config-sweet.git",
  "homepage": "https://github.com/robinloeffel/eslint-config-sweet",
  "bugs": "https://github.com/robinloeffel/eslint-config-sweet/issues",
  "author": "Robin Löffel (https://robinloeffel.ch) <hi@robinloeffel.ch>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "files": [
    "rules",
    "configs"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-unicorn": "^49.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/types": "^18.4.3",
    "@types/eslint": "^8.44.7",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "npm-check-updates": "^16.14.11",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "eslint": "^8",
    "typescript": "^4 || ^5"
