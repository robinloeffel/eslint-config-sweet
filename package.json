{
  "name": "eslint-config-sweet",
  "version": "6.0.0",
  "description": "ESLint shareable config for the most sweet-ass vanilla JavaScript. 🤙🏼",
  "main": "web.js",
  "repository": "https://github.com/robinloeffel/eslint-config-sweet.git",
  "homepage": "https://github.com/robinloeffel/eslint-config-sweet",
  "bugs": "https://github.com/robinloeffel/eslint-config-sweet/issues",
  "author": "Robin Löffel (https://robinloeffel.ch) <hi@robinloeffel.ch>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
    "node",
    "web"
  ],
  "engines": {
    "node": ">=10.12.0"
  },
  "files": [
    "base.js",
    "web.js",
    "node.js"
  ],
  "dependencies": {
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^35.0.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "onchange": "^7.1.0"
  },
  "peerDependencies": {
    "eslint": "^7.32.0"
  },
  "scripts": {
    "start:web": "onchange -i test/web.js index.js web.js -- eslint -c web.js test/web.js",
    "start:node": "onchange -i test/node.js index.js node.js -- eslint -c node.js test/node.js",
    "start:configs": "onchange -i test/*.config.js -- eslint -c web.js test/*.config.js",
    "start": "concurrently 'yarn start:web' 'yarn start:node' 'yarn start:configs'"
  }
}
